  body {
            font-family: helvetica, sans-serif;
			margin:0;
			padding:0;
			box-sizing:border-box;
        }
		nav{
			display:flex;
			justify-content:space-between;
			align-items:center;
			padding:0 50px;
			background:#fff;
			
			
		
		
		}
		.nav-buttons{
			display:flex;
			justify-content:center;
			align-items:center;
		}
		
		.nav-buttons a{
			text-decoration:none;
			margin-left:20px;
		}
		nav h1{
			font-size:40px;
			color:#000000;
			font-weight:500;
		}
		.myContact{
			background:#7877e6;
			color:#fff;
			border-radius:20px 0 20px 0;
			padding:10px 20px;
		}
		
		
		
		
		
		.home{
			display:flex;
			justify-content:center;
			align-items:center;
			width:100%;
			margin-top:70px;
			
		}
		.homeWrap{
			display:flex;
			gap:50px;
			justify-content:center;
			align-items:center;
			
		}
		.homeOne{
			width:400px;
			height:400px;
		}
		.homeOne img{
			width:100%;
			height:100%;
			border-radius:50%;
			object-fit:cover;
			aspect-ratio:1;
		}
		.homeTwo{
			padding:0 45px;
			background:#eaf3f6;
			border-radius:40px;
			display:flex;
			justify-content:center;
			align-items:center;
			flex-direction:column;
			width:600px;
			height:300px;
		}
.homeTwo h1 {
    font-size: 60px;
    color: #000000;
    margin: 0;
    margin-bottom: 50px;
    font-weight: 500;
    position: relative; /* Required for positioning the ::after pseudo-element */
}

.homeTwo h1::after {
    content: "";
    position: absolute;
    bottom: -9px; /* Adjust this value to control the distance between the text and the border */
    left: 0;
    width: 100%;
    height: 7.5px; /* Thickness of the border */
    background-color: #7877e6; /* Color of the border */
}

.homeTwo h1::before {
    content: "";
    position: absolute;
    bottom: -20px; /* Adjust this value to control the distance between the text and the border */
    left: 0;
    width: 100%;
    height: 7.5px; /* Thickness of the border */
    background-color: #7877e6; /* Color of the border */
}

		.homeTwo p{
			color:#172e36;
			font-size:30px;
			letter-spacing:0.2px;
			text-align:center;
			margin:0;
			font-weight:500;
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		  
		
		
		.about{
			display:flex;
			justify-content:center;
			align-items:center;
			width:100%;
			margin-top:70px;
			height:100vh;
		}
		.aboutWrap{
			display:flex;
			gap:20px;
			justify-content:center;
			align-items:center;
			
		}
		.aboutOne{
			width:500px;
			height:500px;
		}
		
		.aboutOne img{
			width:100%;
			height:100%;
			border-radius:30px;
			object-fit:cover;
			aspect-ratio:1;
		}
		.aboutTwo{
			padding:0 45px;
			background:#eaf3f6;
			border-radius:40px;
			display:flex;
			justify-content:flex-start;
			align-items:left;
			flex-direction:column;
			text-align:left;
			width:600px;
			height:500px;
		}
.aboutTwo h1 {
    font-size: 35px;
    color: #000000;
    margin: 0;
    margin-bottom: 35px;
    font-weight: 500;
    position: relative; /* Required for positioning the ::after pseudo-element */
	text-align:left;
	
			padding-top:30px;
			
}

.aboutTwo h1::after {
    content: "";
    position: absolute;
    bottom: 0; /* Adjust this value to control the distance between the text and the border */
    left: 0;
    width: 150px;
    height: 4px; /* Thickness of the border */
    background-color: #7877e6; /* Color of the border */
}

.aboutTwo h1::before {
    content: "";
    position: absolute;
    bottom: -7px; /* Adjust this value to control the distance between the text and the border */
    left: 0;
    width: 150px;
    height: 4px; /* Thickness of the border */
    background-color: #7877e6; /* Color of the border */
}

		.aboutTwo p{
			color:#172e36;
			font-size:18px;
			letter-spacing:0.2px;
			text-align:center;
			margin:0;
			font-weight:400;
			text-align:left;
			line-height:1.4;
			word-spacing:3px;
			font-family:helvetica;
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
			
		.contact{
			display:flex;
			justify-content:center;
			align-items:center;
			width:100%;
			height:100vh;
			background:#eaf3f6;
		}
		.contactWrap{
			display:flex;
			gap:20px;
			justify-content:flex-start;
			align-items:center;
			width:100%;
			
		}
		.contactOne{
		flex-basis:45%;
			display:flex;
			justify-content:flex-start;
			align-items:left;
			flex-direction:column;
			text-align:left;
			padding-left:70px;
			margin-top:-150px;
		}
.contactOne h1 {
    font-size: 55px;
    color: #000000;
    margin: 0;
    margin-bottom: 35px;
    font-weight: 600;
    position: relative; /* Required for positioning the ::after pseudo-element */
	text-align:left;
	
			padding-top:50px;
			
}

.contactOne h1::after {
    content: "";
    position: absolute;
    bottom: -5px; /* Adjust this value to control the distance between the text and the border */
    left: 0;
    width: 500px;
    height: 6px; /* Thickness of the border */
    background-color: #7877e6; /* Color of the border */
}

.contactOne h1::before {
    content: "";
    position: absolute;
    bottom: -14px; /* Adjust this value to control the distance between the text and the border */
    left: 0;
    width: 500px;
    height: 6px; /* Thickness of the border */
    background-color: #7877e6; /* Color of the border */
}

		.contactOne p{
			color:#172e36;
			font-size:18px;
			letter-spacing:0.2px;
			text-align:center;
			margin:0;
			font-weight:400;
			text-align:left;
			line-height:1.4;
			word-spacing:3px;
			font-family:helvetica;
		}
		.icons{
			display:flex;
			gap:10px;
			margin-top:20px;
		}
		.icons i{
			width:50px;
			height:50px;
			text-align:center;
			background:black;
			color:#fff;
			display:flex;
			justify-content:center;
			align-items:center;
			border-radius:50%;
		}
		.contactTwo{
			flex-basis:55%;
			padding-right:70px;
		}
		.contactTwo form{
			display:flex;
			flex-direction:column;
			
		}
		label{
			margin-bottom:10px;
		}
		.inputss{
			padding:10px;
			margin-bottom:20px;
		}
		
		
		
		
		
		
		
		
.portfolio{
			display:flex;
			justify-content:center;
			align-items:center;
			width:100%;
			min-height:100vh;
			padding-bottom:70px;
}
.portfolioWrap{
			display:flex;
			gap:20px;
			justify-content:center;
			align-items:center;
			width:100%;
			flex-direction:column;
			
		}
.portfolioTitle{
	width:100%;
	padding:80px 0;
	background:#eaf3f6;
	text-align:center;
	font-size:40px;
	font-weight:700;
}
.portfolioCard{
	display:flex;
	justify-content:center;
	align-items:center;
			
}
.portfolioCard .portfolioOne{
	flex-basis:50%;
	text-align:center;
}

.portfolioCard .portfolioOne img{
	width:80%;
}

.portfolioCard .portfolioTwo{
	flex-basis:40%;
	text-align:left;
	padding-right:70px;
	margin-left:-50px;
}

.portfolioCard .portfolioTwo h1{
	font-size:28px;
	
}
.portfolioCard .portfolioTwo h3{
	font-size:17px;
	
}
.portfolioCard .portfolioTwo p{
	margin-bottom:70px;
}
.portfolioCard .portfolioTwo a{
	background:#7877e6;
	color:#fff;
	border-radius:20px 0 20px 0;
	padding:20px 40px;
	
}		







	
		
.footer{
			display:flex;
			justify-content:center;
